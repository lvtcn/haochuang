.syk-scrolist { margin-top: 105px; }
.list .li { padding:20px; border-bottom: 0.01em solid #efefef; }
.list .li .name { display: flex; justify-content: space-between; }
.list .li .name .dd1 { color:#fff; width:30px; height:20px; text-align: center; line-height: 20px; margin-right: 5px; background: #ffb142; font-size: 12px; border-radius: 5px; }
.list .li .name .dd2 { flex: 1; color:#000; font-size: 16px; }
.list .li .tist { font-size: 12px; color:#666; margin-top: 15px; display: flex; justify-content: space-between; }
.list .li .tist .hits i { vertical-align: middle; margin-right: 5px; }

