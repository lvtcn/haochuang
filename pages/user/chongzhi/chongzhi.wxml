<!--pages/user/bank/bank.wxml-->
<view class="syk-backfixed">
    <form bindsubmit='formSubmit'>
        <view class="syk-viewbor">
            <view class="tdleft">充值金额</view>
            <view class="tdright">
                <view class="name">
                    <input name='jiage' type='text' placeholder='请输入您要充值的金额' maxlength='8'></input>
                </view>
            </view>
        </view>
        <button formType='submit' class='syk-submit'>提交</button>
    </form>
</view>