<view class="syk-cata {{moreClick.MenuShow}}">    <block wx:for="{{catalist}}" wx:key="*this" wx:index="index">    <view class="text {{num==index?'cur':''}}" bindtap="cataClic"          data-t="{{item.t}}"          data-num='{{index}}'>        {{item.classname}}    </view>    </block>    <block wx:if="{{catalist.length > 4}}">        <view class="jiatou"><i class="iconfont {{moreClick.MenuShow == 'hide'?'icon-next':'icon-xla'}}" bindtap="moreClick" id="moreClick"></i></view>    </block></view><view class="container">    <!--垂直滚动，这里必须设置高度-->    <scroll-view scroll-top="{{scrollTop}}" scroll-y="true" style="height:{{scrollHeight}}px;"                 class="syk-scrolist" bindscrolltolower="bindDownLoad">        <view class="list">            <block wx:if="{{infodata.length == 0 && list == 0}}">                <view class="syk-datanone">                    <image src="/images/wushuju.png" mode="widthFix"></image>                    <view>抱歉，您还没有相关报名信息 ！</view>                </view>            </block>            <block wx:for="{{list}}" wx:key="*this" wx:index="index">                <view class="li">                    <view class="status">                        <view class="i">                            <i class="iconfont icon-delete" bindtap="del" id="{{item.id}}"></i>                        </view>                    </view>                    <view class="top">                        <navigator url="../../{{item.url}}?id={{item.tid.id}}">                            <view class="tdleft">                                <image src="{{item.tid.pic}}" mode="aspectFill"></image>                            </view>                        </navigator>                        <view class="tdright">                            <view class="name">{{item.tid.title}}</view>                        </view>                    </view>                    <view class="orji">                        <view class="order">报名时间：{{item.createtime}}</view>                    </view>                    <view class="buttom">                        <navigator url="../../{{item.url}}?id={{item.tid.id}}">                        <view class="button">                            查看详情                        </view>                        </navigator>                    </view>                </view>            </block>        </view>    </scroll-view></view>